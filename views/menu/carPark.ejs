<div class="form-group p-1">
    <label for="cpark">Car parking</label>
    <select class="form-select" aria-label="Default select example" name="cpark">

        <%for (var i =1;i<=5;i++){%>
            <% if (i===5){%>

                <option><%=i-1+"+"%></option>
                <%}else{%>
                    <option><%=i%></option>
                    <%}%>
            
            <%}%>
    </select>
    <!-- <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> -->
  </div>